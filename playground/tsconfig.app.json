{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "incremental": false,
    "composite": false,
    "rootDir": "..",
    "paths": {
      "@video-editor/ui": [
        "./src/shims-video-editor-ui.d.ts"
      ],
      "@video-editor/ui/*": [
        "./src/shims-video-editor-ui.d.ts"
      ],
      "@video-editor/*": [
        "../packages/*/src/index.ts"
      ]
    },
    "typeRoots": [
      "../types",
      "../node_modules/@types"
    ],
    "types": [
      "node",
      "vite/client",
      "../types/mp4box.d.ts"
    ]
  },
  "include": [
    "src",
    "auto-imports.d.ts"
  ],
  "exclude": [
    "dist"
  ]
}
